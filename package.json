    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:scss --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "eleventy-plugin-lazyimages": "^1.1.2"
  }
}
