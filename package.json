{
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.5",
    "fs": "^0.0.1-security",
    "jpeg-js": "^0.4.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.37.0",
    "three.js": "^0.77.0"
  },
  "scripts": {
    "watch:scss": "sass --watch scss/style.scss:css/style.css",
